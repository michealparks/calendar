@font-face
  font-family "Helvetica Neue"
  src url("helvetica_neue.woff")
  
@font-face
  font-family 'icomoon'
  src url('icomoon.woff') format('woff')
  font-weight normal
  font-style normal

[class^="icon-"], [class*=" icon-"]
  font-family 'icomoon'
  speak none
  font-style normal
  font-weight normal
  font-variant normal
  text-transform none
  line-height 1

  /* Better Font Rendering =========== */
  -webkit-font-smoothing antialiased
  -moz-osx-font-smoothing grayscale

.icon-edit:before
  content "\e603"
.icon-trash:before
  content "\e109"
