#view-interpreters
  
  .color
    position relative
    display inline-block
    height 10px
    width 10px
    margin 0 10px 0 0
    border-radius 100%
    
  .header .text
      
    &:nth-child(2)
      position absolute
      left 50%
    
    &:last-child
      float right
        
  .container-interpreters .text
        
    &:nth-child(3)
      position absolute
      left 50%
      
    &:nth-child(4)
      float right
      
    &:last-child
      float right
        
    
  