#view-nav
  z-index 1
  position fixed
  top 0
  left 0
  width 100%
  height 40px
  background-color #eee
  color #666
  
  .li
    user-select none
    cursor pointer
    line-height 40px
    display inline-block
    margin 0 0 0 20px
    font-size 14px
    
    &:last-child
      margin 0 20px 0 0 
    
  .logout
    position absolute
    right 0
    top 0